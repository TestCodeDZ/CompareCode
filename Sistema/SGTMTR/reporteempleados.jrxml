	<queryString language="SQL">
		<![CDATA[SELECT
     u.`ID` AS u_ID,
     u.`Nombres` AS u_Nombres,
     u.`Apellidos` AS u_Apellidos,
     tu.`TipoUser` AS tu_TipoUser,
     s.`NombreSucursal` AS s_NombreSucursal,
     u.`Usuario` AS u_Usuario
FROM
     `usuarios` u,
     `tipousuario` tu,
